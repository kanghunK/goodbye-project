{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base",
		"prettier",
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"plugins": ["moment", "@emotion"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"no-use-before-define": [
			"error",
			{
				"variables": false
			}
		]
	}
}
